<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_feed">

    <fragment
        android:id="@+id/navigation_feed"
        android:name="devandroid.micaela.moldylemons.ui.feed.FeedFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_feed" />

    <fragment
        android:id="@+id/navigation_review_list"
        android:name="devandroid.micaela.moldylemons.ui.reviewList.ReviewListFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_review_list" />

    <fragment
        android:id="@+id/navigation_wishlist"
        android:name="devandroid.micaela.moldylemons.ui.wishlist.WishlistFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_wishlist" />
